import java.util.ArrayList;
import java.util.List;


public class Order {
      List<Book> books;
      public Order()
      {
    	  books =  new ArrayList<>();
    	  books.add(new DownloadableBook("2345", 10,  "75087"));
    	  books.add(new HardCoverBook("2388", 20,  "75087", "GROUND"));
      }
      
     
      
     
}
